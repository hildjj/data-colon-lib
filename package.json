{
  "name": "data-colon-lib",
  "version": "0.0.0",
  "description": "Create and parse data URIs",
  "main": "lib/index.js",
  "type": "module",
  "scripts": {
    "parser": "peggy -c peggy.config.js ",
    "prebuild": "npm run parser",
    "build": "tsup",
    "lint": "eslint .",
    "pretest": "npm run build -- --sourcemap inline",
    "test": "c8 ava",
    "posttest": "tsup --silent",
    "docs": "typedoc",
    "clean": "rimraf docs lib coverage src/grammar.js src/grammar.d.ts"
  },
  "keywords": [
    "rfc2397",
    "rfc2396",
    "datacolon",
    "data",
    "uri",
    "url",
    "parser",
    "mime"
  ],
  "author": "Joe Hildebrand <joe-github@cursive.net>",
  "license": "MIT",
  "files": [
    "lib/"
  ],
  "dependencies": {
    "uint8array-extras": "1.4.0"
  },
  "devDependencies": {
    "@cto.af/eslint-config": "5.1.13",
    "@types/node": "22.13.9",
    "@typescript-eslint/eslint-plugin": "8.26.0",
    "@typescript-eslint/parser": "8.26.0",
    "ava": "6.2.0",
    "c8": "10.1.3",
    "eslint": "9.21.0",
    "eslint-plugin-jsdoc": "50.6.3",
    "eslint-plugin-markdown": "5.1.0",
    "eslint-plugin-node": "11.1.0",
    "peggy": "github:peggyjs/peggy",
    "rimraf": "6.0.1",
    "ts-node": "10.9.2",
    "tsup": "8.4.0",
    "typedoc": "0.27.9",
    "typescript": "5.8.2",
    "typescript-eslint": "8.26.0"
  },
  "packageManager": "pnpm@10.6.0",
  "engines": {
    "node": ">=18"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
