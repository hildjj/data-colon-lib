{
  "name": "data-colon-lib",
  "version": "0.0.0",
  "description": "Create and parse data URIs",
  "main": "lib/index.js",
  "type": "module",
  "scripts": {
    "build": "peggy --format es --allowed-start-rules dataurl,mediatype src/grammar.peggy -o src/grammar.js && tsc",
    "map": "peggy -m inline --format es --allowed-start-rules dataurl,mediatype src/grammar.peggy -o src/grammar.js && tsc --sourceMap",
    "lint": "eslint .",
    "test": "c8 -r lcov ava",
    "clean": "rimraf lib coverage src/grammar.js",
    "watch": "swc -s inline -d lib src -w"
  },
  "keywords": [
    "rfc2397",
    "2396",
    "datacolon",
    "data",
    "parser",
    "mime"
  ],
  "author": "Joe Hildebrand <joe-github@cursive.net>",
  "license": "MIT",
  "files": [
    "lib/"
  ],
  "devDependencies": {
    "@cto.af/eslint-config": "5.1.13",
    "@swc/cli": "0.6.0",
    "@swc/core": "1.11.7",
    "@types/node": "22.13.9",
    "@typescript-eslint/eslint-plugin": "8.26.0",
    "@typescript-eslint/parser": "8.26.0",
    "ava": "6.2.0",
    "c8": "10.1.3",
    "eslint": "9.21.0",
    "eslint-plugin-jsdoc": "50.6.3",
    "eslint-plugin-markdown": "5.1.0",
    "eslint-plugin-node": "11.1.0",
    "peggy": "github:peggyjs/peggy",
    "rimraf": "6.0.1",
    "ts-node": "10.9.2",
    "typescript": "5.8.2",
    "typescript-eslint": "8.26.0"
  },
  "packageManager": "pnpm@10.6.0+sha512.df0136e797db0cfa7ec1084e77f3bdf81bacbae9066832fbf95cba4c2140ad05e64f316cde51ce3f99ea00a91ffc702d6aedd3c0f450f895e3e7c052fe573cd8"
}
